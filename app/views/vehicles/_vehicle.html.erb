<div id="<%= dom_id vehicle %>">
  <p>
    <strong>Manufacturer:</strong>
    <%= vehicle.manufacturer %>
  </p>

  <p>
    <strong>Model:</strong>
    <%= vehicle.model %>
  </p>

  <p>
    <strong>Colour:</strong>
    <%= vehicle.colour %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= vehicle.price %>
  </p>

  <p>
    <strong>Odometer:</strong>
    <%= vehicle.odometer %>
  </p>

  <p>
    <strong>Vehicle type:</strong>
    <%= vehicle.vehicle_type %>
  </p>

</div>
